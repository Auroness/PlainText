  "triggers": { "onattack": "(function(attacker) {\n    if (this.enemies.indexOf(attacker) === -1) {\n        this.enemies.push(attacker);\n    }\n\n    var currentRoomId = this.currentRoom.id;\n    function go() {\n        this.go(this.currentRoom.exits.filter(function(exit) {\n            return !exit.hidden && !exit.door && exit.name !== 'gate' && exit.destination.id != currentRoomId;\n        }).randomElement());\n    }\n\n    this.setTimeout(function() {\n        go.call(this);\n        this.setTimeout(go, 600);\n    }, 200);\n})", "oncharacterentered": "(function(activator) {\n    var index = this.enemies.indexOf(activator);\n    if (index > -1) {\n        this.setTimeout(function() {\n            if (activator.currentRoom.id !== this.currentRoom.id) {\n                return;\n            }\n\n            activator.send('The cat hisses to you.');\n\n            var characters = this.currentRoom.characters;\n            this.currentRoom.players.forEach(function(player) {\n                if (player.id !== activator.id) {\n                    player.send('The cat hisses at ' + activator.definiteName(characters) + '.');\n                }\n            });\n            this.enemies.splice(index, 1);\n        }, 600);\n    }\n})", "oncharacterexit": "(function(activator, exitName) {\n    if (activator.name === 'housewife' && exitName) {\n        if (randomInt(0, 3) < 2) {\n            this.setTimeout(function() {\n                this.go(this.currentRoom.exits.named(exitName));\n            }, 0);\n        }\n    }\n})", "onspawn": "(function() {\n    this.enemies = [];\n\n    this.setInterval(function() {\n        if (randomInt(0, 2) == 0) {\n            var exits = this.currentRoom.exits.filter(function(exit) {\n                return !exit.hidden && (!exit.door || exit.open) && exit.name !== 'gate'\n            });\n            if (exits.length > 0) {\n                this.go(exits.randomElement());\n            }\n        }\n    }, 40000);\n})" },

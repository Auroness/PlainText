  "triggers": { "onattack": "(function(attacker) {\n    this.shout('Help! Guards!');\n})", "onspawn": "(function() {\n    var EMPTY_DESCRIPTION = \"She's carrying an empty basket.\";\n    var FILLING_DESCRIPTION = \"She's busy getting water from the well.\";\n    var FULL_DESCRIPTION = \"She's carrying a bucket full of water.\";\n\n    if (this.currentArea) {\n        this.currentArea.removeNPC(this);\n    }\n    this.enter($('area:132'));\n    this.description = EMPTY_DESCRIPTION;\n\n    var bucket = this.inventory[0];\n\n    var self = this;\n    function pet() {\n        self.setTimeout(function() {\n            if (this.currentArea.npcs.named('cat')) {\n                this.currentArea.players.forEach(function(player) {\n                    player.send(\"The housewife pets the cat.\"); \n                });\n            }\n        }, 150);\n    }\n\n    this.setInterval(function() {\n        if (this.currentArea === null || (this.currentArea.id === 132 && !this.currentArea.npcs.contains(this))) {\n            this.description = EMPTY_DESCRIPTION;\n\n            bucket.name = \"empty bucket\";\n            bucket.plural = \"empty buckets\";\n            bucket.indefiniteArticle = \"an\";\n            bucket.description = \"It's a bucket. It's empty.\";\n            bucket.weight = 1;\n\n            if (this.currentArea === null) {\n                this.enter($('area:132'));\n                return;\n            }\n        }\n\n        switch (this.currentArea.id) {\n            case 132:\n                if (this.description === EMPTY_DESCRIPTION) {\n                    this.go(this.currentArea.exits.named('west'));\n                    pet();\n                } else {\n                    this.leave(this.currentArea);\n                    this.currentArea = null;\n                }\n                break;\n            case 113:\n                if (this.description === EMPTY_DESCRIPTION) {\n                    this.go(this.currentArea.exits.named('west'));\n                    pet();\n                } else {\n                    this.go(this.currentArea.exits.named('east'));\n                }\n                break;\n            case 110:\n                if (this.description === EMPTY_DESCRIPTION) {\n                    this.description = FILLING_DESCRIPTION;\n                } else if (this.description === FILLING_DESCRIPTION) {\n                    this.description = FULL_DESCRIPTION;\n\n                    bucket.name = \"bucket full of water\";\n                    bucket.plural = \"buckets full of water\";\n                    bucket.indefiniteArticle = \"a\";\n                    bucket.description = \"It's a bucket. It's filled with water.\";\n                    bucket.weight = 10;\n                } else {\n                    this.go(this.currentArea.exits.named('east'));\n                }\n                break;\n        }\n    }, 27768);\n})" },

  "triggers": { "onattack": "(function(attacker) {\n    this.enemies.insert(attacker);\n\n    this.setTimeout(function() {\n        this.shout(\"Help! Guards!\");\n    }, 150);\n})", "oncharacterentered": "(function(activator) {\n    var room = this.currentRoom;\n    var characters = room.characters;\n    if (activator.name.contains(\"city guard\") && characters.containsAny(this.enemies)) {\n        var enemy = this.enemies.findFirstAmong(characters);\n        this.setTimeout(function() {\n            if (!room.players.isEmpty() && !this.pointed) {\n                room.players.send(this.definiteName(room.characters).capitalized() + \" points to \" + enemy.definiteName(characters) + \".\");\n                this.pointed = true;\n            }\n    \n            activator.enemies.insert(enemy);\n            this.setTimeout(function() {\n                activator.invokeTrigger(\"onactive\");\n                this.pointed = false;\n            }, randomInt(100, 400));\n        }, 10);\n    }\n})", "onspawn": "(function() {\n    var EMPTY_DESCRIPTION = \"She's carrying an empty basket.\";\n    var FILLING_DESCRIPTION = \"She's busy getting water from the well.\";\n    var FULL_DESCRIPTION = \"She's carrying a bucket full of water.\";\n\n    if (this.currentRoom) {\n        this.currentRoom.removeCharacter(this);\n    }\n    this.enter($('room:132'));\n    this.description = EMPTY_DESCRIPTION;\n    this.enemies = [];\n\n    var bucket = this.inventory[0];\n    if (!bucket) {\n        bucket = Realm.createObject('item');\n        bucket.name = \"empty bucket\";\n        bucket.description = \"It's a bucket. It's empty.\";\n        bucket.weight = 1;\n        bucket.portable = true;\n        this.addInventoryItem(bucket);\n    }\n\n    var self = this;\n    function pet() {\n        self.setTimeout(function() {\n            if (this.currentRoom && this.currentRoom.characters.named('cat')) {\n                this.currentRoom.players.forEach(function(player) {\n                    player.send(\"The housewife pets the cat.\"); \n                });\n            }\n        }, 150);\n    }\n\n    this.setInterval(function() {\n        if (this.currentRoom === null || (this.currentRoom.id === 132 && !this.currentRoom.characters.contains(this))) {\n            this.description = EMPTY_DESCRIPTION;\n\n            bucket.name = \"empty bucket\";\n            bucket.description = \"It's a bucket. It's empty.\";\n            bucket.weight = 1;\n\n            if (this.currentRoom === null) {\n                this.enter($('room:132'));\n                return;\n            }\n        }\n\n        switch (this.currentRoom.id) {\n            case 132:\n                if (this.description === EMPTY_DESCRIPTION) {\n                    this.go(this.currentRoom.exits.named('west'));\n                    pet();\n                } else {\n                    this.leave(this.currentRoom);\n                    this.currentRoom = null;\n                }\n                break;\n            case 113:\n                if (this.description === EMPTY_DESCRIPTION) {\n                    this.go(this.currentRoom.exits.named('west'));\n                    pet();\n                } else {\n                    this.go(this.currentRoom.exits.named('east'));\n                }\n                break;\n            case 110:\n                if (this.description === EMPTY_DESCRIPTION) {\n                    this.description = FILLING_DESCRIPTION;\n                } else if (this.description === FILLING_DESCRIPTION) {\n                    this.description = FULL_DESCRIPTION;\n\n                    bucket.name = \"bucket full of water\";\n                    bucket.plural = \"buckets full of water\";\n                    bucket.description = \"It's a bucket. It's filled with water.\";\n                    bucket.weight = 10;\n                } else {\n                    this.go(this.currentRoom.exits.named('east'));\n                }\n                break;\n        }\n    }, 27768);\n})" },
